{
  "name": "ui-driver",
  "version": "0.1.0",
  "description": "A driver for connecting material-components to cerebral",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src test",
    "dev": "parallelshell 'babel src --watch --out-dir lib' 'watch --wait=1 \"eslint src test && mocha --compilers js:babel/register --recursive --reporter dot\" src test'",
    "prebuild": "npm run lint",
    "build": "babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "mocha --compilers js:babel/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/garth/ui-driver.git"
  },
  "keywords": [
    "cerebral",
    "material-components"
  ],
  "author": "Garth Williams",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/garth/ui-driver/issues"
  },
  "homepage": "https://github.com/garth/ui-driver#readme",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.5",
    "baobab": "^2.1.1",
    "cerebral": "^0.25.1",
    "cerebral-baobab": "^0.4.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.1",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "parallelshell": "^2.0.0",
    "watch": "^0.16.0"
  },
  "dependencies": {},
  "peerDependencies": {
    "moment": "^2.10.6"
  }
}
