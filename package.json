    "url": "https://github.com/garth/ui-driver/issues"
  },
  "homepage": "https://github.com/garth/ui-driver#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "baobab": "^2.1.1",
    "cerebral": "^0.28.2",
    "cerebral-baobab": "^0.4.2",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.4",
    "moment": "^2.10.6",
    "parallelshell": "^2.0.0",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "email-validator": "^1.0.3"
